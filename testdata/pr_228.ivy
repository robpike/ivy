# Argument with prec < conf.floatPrec
)prec 64
a = float 2
)prec 128
(sqrt a) == sqrt 2
	1

# Argument with prec > conf.floatPrec
)prec 256
a = float 2
)prec 128
(sqrt a) == sqrt 2
	1

# Same test for BigFloat.Sprint
)prec 64
a = float 1 << 10000
)prec 128
)format "%.30g"
a
	1.99506311688075838488374216268e+3010 
)format ""
)prec 256
