# Binary operations with big floats.

)format "%.16g"
1 + (sqrt 2)
	2.414213562373095

)format "%.16g"
1 - (sqrt 2)
	-0.414213562373095

)format "%.16g"
2 * (sqrt 2)
	2.82842712474619

)format "%.16g"
1/sqrt 2
	0.7071067811865475

(sqrt 2) == (sqrt iota 3)
	0 1 0

(sqrt 2) != (sqrt iota 3)
	1 0 1

(sqrt 2) < (sqrt iota 3)
	0 0 1

(sqrt 2) <= (sqrt iota 3)
	0 1 1

(sqrt 2) > (sqrt iota 3)
	1 0 0

(sqrt 2) >= (sqrt iota 3)
	1 1 0

2 log 2**32
	32

# Was bug; overwrote arguments. Issue 30.
e**pi
e
pi
	23.1406926328
	2.71828182846
	3.14159265359

(float 0) ** 1e100
	0

(float 1) ** 1e100
	1

(sqrt 0) and sqrt 2
	0

(sqrt 2) and sqrt 2
	1

(sqrt 0) or sqrt 2
	1

(sqrt 2) or sqrt 2
	1

(sqrt 0) xor sqrt 2
	1

(sqrt 2) xor sqrt 2
	0

(sqrt 0) nor sqrt 2
	0

(sqrt 2) nor sqrt 2
	0

(sqrt 0) nand sqrt 2
	1

(sqrt 2) nand sqrt 2
	0

(sqrt 2) min sqrt 3
	1.41421356237

(sqrt 2) max sqrt 3
	1.73205080757

